{
  "name": "twitter-search-app",
  "version": "0.1.0",
  "scripts": {
    "dbUp": "db-migrate up --config ./config/database.json -e dev",
    "eslint": "eslint ./source/**/*.js* --fix",
    "clean": "rm -rf ./build",
    "buildClient": "webpack",
    "buildServer": "babel ./ico --out-dir ./build/media --copy-files && babel ./source/server --out-dir ./build/server --copy-files && babel ./config --out-dir ./build/config --copy-files",
    "build": "npm run clean; npm run buildClient; npm run buildServer",
    "nodemonServer": "nodemon ./source/server/src/app.js --exec babel-node --presets env,react --plugins transform-object-rest-spread --watch source/server",
    "start": "NODE_ENV=prod concurrently --kill-others \"npm run start:client\" \"npm run start:server\"",
    "start:server": "npm run buildServer && npm run nodemonServer",
    "start:client": "webpack --watch --progress",
    "start:dev": "NODE_ENV=dev concurrently --kill-others \"npm run start:client\" \"npm run start:server\"",
    "start:old": "npm run clean && npm run buildServer && node build/server/app.js",
    "server": "node ./build/server/src/app.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Serhii Ivanchecnko",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Serhii_Ivanchenko/treasure#readme",
  "babel": {
    "presets": [
      "es2015",
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.0",
    "connect-mongodb-session": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "db-migrate": "^0.10.2",
    "ejs": "^2.5.7",
    "email-validator": "^1.1.1",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.16.2",
    "express-mongo-sanitize": "^1.3.2",
    "express-sanitizer": "^1.0.2",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fs-extra-promise": "^1.0.1",
    "helmet": "^3.9.0",
    "log4js": "^2.3.12",
    "md5": "^2.2.1",
    "mongoose": "^4.13.6",
    "node-sass": "^4.8.3",
    "nodemailer": "^4.4.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "open-browser-webpack-plugin": "0.0.5",
    "prop-types": "latest",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-highcharts": "^16.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "3.2.1",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.4",
    "style-loader": "^0.19.1",
    "twitter": "^1.7.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "nodemon": "^1.14.11",
    "testcafe": "^0.18.6",
    "testcafe-react-selectors": "^1.0.5"
  }
}
